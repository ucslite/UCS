<!-- BEGIN RequisitionLists_UI.jspf -->		
<span class="spanacce" id="requisitionList_widget_ACCE_Label" aria-hidden="true"><fmt:message key="ACCE_REGION_REQUISITIONLIST" bundle="${widgetText}" /></span>

<%-- Check for an upload CSV error --%>
<c:if test="${!empty errorMessage}">
	<fmt:message bundle="${storeText}" var ="msgType" key="ERROR_MESSAGE_TYPE"/>
	<c:set var = "errorMessage" value ="${msgType}${errorMessage}"/>
	<script type="text/javascript">
		dojo.addOnLoad(function() { 
			dojo.byId('MessageArea').style.display = "block";
			dojo.byId('ErrorMessageText').innerHTML =<wcf:json object="${errorMessage}"/>;
			dojo.byId('MessageArea').focus();
			setTimeout("dojo.byId('ErrorMessageText').focus()",2000);
		});
	</script>
</c:if>

<div dojoType="wc.widget.RefreshArea" widgetId="RequisitionListTable_Widget" id="RequisitionListTable_Widget" controllerId="RequisitionListTable_controller" role="region" aria-labelledby="requisitionList_widget_ACCE_Label" ariaMessage="<fmt:message key="ACCE_STATUS_REQUISITIONLIST_UPDATED" bundle="${widgetText}" />">					
	<%@include file="/Widgets_701/Common/MyAccountList/ListTable_UI.jspf" %>		
</div>									
<!-- END RequisitionList_UIs.jspf -->
